Sub OpenRemote
    Set oDispatch = CreateUnoService(&quot;com.sun.star.frame.DispatchHelper&quot;)
    Dim args(0) As New com.sun.star.beans.PropertyValue
    oDispatch.executeDispatch(StarDesktop.ActiveFrame, &quot;mdrive:Open&quot;, &quot;&quot;, 0, args)
End Sub

